  "steps": [
    {
      "step": "Boil a kettle, finely chop onion, carrot and garlic",
      "imgSrc": "step.jpg"
    },
    {
      "step": "Fry the onion and carrot with a pinch of salt for 4 mins",
      "imgSrc": "step.jpg"
    },
    { "step": "Add the mince beef and cook for further 4 mins", "imgSrc": "step.jpg" },
    {
      "step": "While the beef is cooking add the boiled water to a pan and cook the pasta for 10 mins",
      "imgSrc": "step.jpg"
    },
    {
      "step": "Dissolve the beef stock cube with the soy sauce in 200ml boiled water",
      "imgSrc": "step.jpg"
    },
    {
      "step": "Add the garlic, tomato paste, dried basil, dried oregano, stock mix to the pan",
      "imgSrc": "step.jpg"
    },
    { "step": "Season with pepper and cook for further 4 mins", "imgSrc": "step.jpg" },
    {
      "step": "Mix the pasta into the pan and serve with grated cheese",
      "imgSrc": "step.jpg"
    }
